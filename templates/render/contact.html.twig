{% extends "base.html.twig" %}

{% block title %}Contact
{% endblock %}

{% block body %}
	
	<h1 class="text-center my-3">Contact</h1>
	

	<div class="row">
		<div class=" col-6 mx-auto my-3">
			{{ form_start(form) }}

			<div class="row">

				<div class="col-6">

					{{ form_row(form.prenom) }}

				</div>

				<div class="col-6">

					{{ form_row(form.nom) }}

				</div>

			</div>

			{{ form_end(form) }}

		</div>

	</div>

{% endblock %}